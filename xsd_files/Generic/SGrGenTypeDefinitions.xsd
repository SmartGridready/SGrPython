  <complexType name="SGrScalingType">
    <annotation>
      <documentation>scaled_value = value * multiplicator * 10^powerof10
				This type is used for to convert intereger datapoint values into
				floats only
			</documentation>
    </annotation>
    <sequence>
      <element name="multiplicator" type="int" />
      <element name="powerof10" type="int" />
    </sequence>
  </complexType>

