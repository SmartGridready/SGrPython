  <complexType name="SGrReleaseNotes">
    <annotation>
      <documentation xml:lang="en">
        Contains versioning, history and release states
      </documentation>
    </annotation>
    <sequence>
      <element maxOccurs="1" minOccurs="1" name="state" type="sgr:SGrReleaseState" />
      <element maxOccurs="1" minOccurs="0" name="remarks" type="string" />
      <element maxOccurs="unbounded" minOccurs="1" name="changelog" type="sgr:SGrChangeLog" />
    </sequence>
  </complexType>
  <simpleType name="SGrReleaseState">
    <restriction base="string">
      <enumeration value="Draft" />
      <enumeration value="Review" />
      <enumeration value="Published" />
      <enumeration value="Revoked" />
    </restriction>
  </simpleType>
  <complexType name="SGrChangeLog">
    <sequence>
      <element name="version" type="string" />
      <element name="date" type="date" />
      <element name="author" type="string" />
      <element name="comment" type="string" />
    </sequence>
  </complexType>
  <simpleType name="SGrSpecialQualityReqType">
    <annotation>
      <documentation xml:lang="en">the special quality requirement
				describes a test to be applied like a quality certification for
				SmartMeters
			</documentation>
      <documentation xml:lang="de" />
    </annotation>
    <restriction base="string" />
  </simpleType>
  <!-- data value with precision range -->
  <simpleType name="SGrPrecisionType">
    <annotation>
      <documentation xml:lang="en"> Precision defines the accuracy
				of a data point either related to a measurement value, a simulation
				result or of a setpoint value
			</documentation>
      <documentation xml:lang="de"> Der Parameter Präzision
				definiert die Genauigkeit eines Messwertes, einer Simulation oder
				einer Stellgrössenumsetzung
			</documentation>
    </annotation>
    <restriction base="double">
      <minInclusive value="0.001" />
      <maxInclusive value="15" />
    </restriction>
  </simpleType>
  <!-- Namelist Type for listing the relevant name references -->
  <complexType name="SGrNamelistType">
    <annotation>
      <documentation xml:lang="en">a list of relevant namespaces
				list for to display names used in different standards like EEBUS,
				IEC6850,, SAREF4ENER etc this list type is used for devices,
				functional profiles and datapoints. This namespace naming framework
				is typically used by the SGr association in order to tailer web
				based information tools. This complex data type is intended to be
				extended for future relevant standarrds in order to secure
				information over technology live cycles
			</documentation>
      <documentation xml:lang="de"> List der für ein Profil
				relevanten Namen von benutzten Standards
			</documentation>
    </annotation>
    <sequence maxOccurs="1" minOccurs="1">
      <element maxOccurs="1" minOccurs="0" name="sLV1Name" type="string">
        <annotation>
          <documentation>names used for SGr label V1 used to secure legacy
						compatibility
					</documentation>
        </annotation>
      </element>
      <element maxOccurs="1" minOccurs="0" name="sWorkName" type="string">
        <annotation>
          <documentation>work names for temporary use</documentation>
        </annotation>
      </element>
      <element maxOccurs="1" minOccurs="0" name="sManufName" type="string">
        <annotation>
          <documentation>manufacturers may use an internal wording
					</documentation>
        </annotation>
      </element>
      <element maxOccurs="1" minOccurs="0" name="sIEC61850Name" type="string">
        <annotation>
          <documentation>IEC 61850 termonoligy place to add the 61850
						abreviatuions if an overlap exists
					</documentation>
        </annotation>
      </element>
      <element maxOccurs="1" minOccurs="0" name="sSAREFName" type="string">
        <annotation>
          <documentation>SAREF for ENER termonoligy place to add the SAREF
						abreviations if an overlap exists
					</documentation>
        </annotation>
      </element>
      <element maxOccurs="1" minOccurs="0" name="sEEBUSName" type="string">
        <annotation>
          <documentation>EEBUS for terminology place to add the EEBUS
						abreviations if an overlap exists
					</documentation>
        </annotation>
      </element>
      <element maxOccurs="1" minOccurs="0" name="sHPbwpName" type="string">
        <annotation>
          <documentation>bwp (German Heat Pump Association) for terminology place to add the bwp
						naming for HVAC if an overlap exists
		  </documentation>
        </annotation>
      </element>
    </sequence>
  </complexType>

  <!-- the basic data based used für the generic level of a communicator -->
  <complexType name="SGrBasicGenDataPointTypeType">
    <annotation>
      <documentation xml:lang="en"> These are the basic generic
				data types must remain high level definitions for to be supported.
				The definition focuses on the programming languages used at the
				level of the communicators. So far Java and Python.
			</documentation>
      <documentation xml:lang="de" />
      <documentation />
    </annotation>
    <choice>
      <element maxOccurs="1" minOccurs="1" name="boolean" type="boolean" />
      <element maxOccurs="1" minOccurs="1" name="int8" type="byte" />
      <element maxOccurs="1" minOccurs="1" name="int16" type="short" />
      <element maxOccurs="1" minOccurs="1" name="int32" type="integer" />
      <element maxOccurs="1" minOccurs="1" name="int64" type="long" />
      <element maxOccurs="1" minOccurs="1" name="int8U" type="unsignedByte" />
      <element maxOccurs="1" minOccurs="1" name="int16U" type="unsignedShort" />
      <element maxOccurs="1" minOccurs="1" name="int32U" type="unsignedInt" />
      <element maxOccurs="1" minOccurs="1" name="int64U" type="unsignedLong" />
      <element maxOccurs="1" minOccurs="1" name="float32" type="float" />
      <element maxOccurs="1" minOccurs="1" name="float64" type="double" />
      <element maxOccurs="1" minOccurs="1" name="enum" type="sgr:SGrEnumListType" />
      <element maxOccurs="1" minOccurs="1" name="dateTime" type="dateTime" />
      <element maxOccurs="1" minOccurs="1" name="string" type="string" />
      <!-- weitere aus IEC61850 übertragbare Datentypen: Achtung: Elemente müssen ohne "_" sein und mit Kleinbuschtsaben beginnen
        		     <enumeration value="Dbpos"/> 
					<enumeration value="Tcmd"/> <enumeration value="Quality"/> <enumeration value="VisString32"/> 
					<enumeration value="VisString64"/> <enumeration value="VisString65"/> <enumeration 
					value="VisString129"/> <enumeration value="VisString255"/> <enumeration value="Octet64"/> 
					<enumeration value="Unicode255"/> <enumeration value="Struct"/> <enumeration 
					value="EntryTime"/> <enumeration value="Check"/> <enumeration value="ObjRef"/> 
					<enumeration value="Currency"/> <enumeration value="PhyComAddr"/> <enumeration 
					value="TrgOps"/> <enumeration value="OptFlds"/> <enumeration value="SvOptFlds"/> 
					<enumeration value="LogOptFlds"/> <enumeration value="EntryID"/> <enumeration 
					value="Octet6"/> <enumeration value="Octet16"/> -->
    </choice>
  </complexType>

  <complexType name="SGrBasicGenArrayDPTypeType">

    <annotation>
      <documentation xml:lang="en"> These are the basic generic
				data array  types must remain high level definitions for to be supported.
				The definition focuses on the programming languages used at the
				level of the communicators. So far Java and Python.
			</documentation>
      <documentation xml:lang="de" />
      <documentation />
    </annotation>
    <sequence>
      <element name="type" type="sgr:SGrBasicGenDataPointTypeType" />
      <element name="lenght" type="integer" />
    </sequence>
  </complexType>
  <!-- enumeration to select mandatory, optional or recommended use of elements -->
  <simpleType name="SGrMROPresenceLevelIndicationType">
    <annotation>
      <documentation xml:lang="en"> Names for the presence
				indication of Elements listed in the tables are defined as follows
				(using the EEBUS terminology): "M" mandatotory use (IEC:SHALL), "R"
				recommended use (IEC:SHOULD) and "O" optional use (IEC:MAY)
			</documentation>
      <documentation xml:lang="de" />
      <documentation />
    </annotation>
    <restriction base="Name">
      <enumeration value="M" />
      <enumeration value="R" />
      <enumeration value="O" />
    </restriction>
  </simpleType>
  <!-- enumeration to select data direction (Read Write) and persistent storage 
		use of elements -->
  <simpleType name="SGrRWPType">
    <annotation>
      <documentation xml:lang="en"> Names for the data direction
				indication. "R" read only data, "W" rite only data, "RW" readible
				and writable data, "RWP" readible and writable data with persistent
				storage
			</documentation>
      <documentation xml:lang="de" />
      <documentation />
    </annotation>
    <restriction base="Name">
      <enumeration value="R" />
      <enumeration value="W" />
      <enumeration value="RW" />
      <enumeration value="RWP" />
    </restriction>
  </simpleType>
  <!-- create a new title as separator in between type definitions -->
  <complexType name="SGrLegibDocumentationType">
    <annotation>
      <documentation>This element us used to extend the definitions with
				legible text elements: a short understandbale explanation of the
				items addressed. These elements are used for printed and published
				information
			</documentation>
    </annotation>
    <sequence>
      <element name="textElement">
        <annotation>
          <documentation>information to be published</documentation>
        </annotation>
        <simpleType>
          <restriction base="string">
            <minLength value="0" />
            <maxLength value="4000" />
          </restriction>
        </simpleType>
      </element>
      <element name="language" type="sgr:SGrLanguageType">
        <annotation>
          <documentation>language identifier de, en, fr, it</documentation>
        </annotation>
      </element>
      <element maxOccurs="1" minOccurs="0" name="uri" type="anyURI">
        <annotation>
          <documentation>URI pointong towards additional information
					</documentation>
        </annotation>
      </element>
    </sequence>
  </complexType>
  <simpleType name="SGrLanguageType">
    <annotation>
      <documentation>this is the identification of the language for
				information to be published. So far en, de, fr and it is possible
			</documentation>
    </annotation>
    <restriction base="Name">
      <enumeration value="de" />
      <enumeration value="en" />
      <enumeration value="fr" />
      <enumeration value="it" />
    </restriction>
  </simpleType>
  <complexType name="SGrVersionNumberType">
    <annotation>
      <documentation>a three digit version mumber system.
